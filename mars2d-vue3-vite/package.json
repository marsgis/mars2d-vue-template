{
  "name": "mars2d-vue-template",
  "version": "3.2.0",
  "description": "Mars2D平台,Vue最简项目模版",
  "scripts": {
    "dev": "vite",
    "serve": "npm run clean-cache && vite --host",
    "clean-cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean-lib": "rimraf node_modules",
    "preview": "vite preview",
    "build": "npm run lint && vite build",
    "serve:dist": "http-server ./dist",
    "lint": "vue-tsc --noEmit --skipLibCheck && npm run eslint",
    "eslint": "eslint ./src/**/*.{js,jsx,vue,ts,tsx} --fix"
  },
  "dependencies": {
    "mars2d": "~3.2.0",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@types/leaflet": "^1.7.1",
    "@types/node": "^17.0.21",
    "@vitejs/plugin-vue": "^2.1.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "consola": "^2.15.3",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-vue": "^8.3.0",
    "fs-extra": "^10.0.0",
    "http-server": "^14.1.0",
    "less": "^4.1.2",
    "rollup-plugin-external-globals": "^0.6.1",
    "serve-static": "^1.14.2",
    "standard": "^16.0.4",
    "typescript": "4.5.5",
    "unplugin-vue-components": "^0.17.13",
    "vite": "^2.8.6",
    "vite-plugin-eslint": "^1.3.0",
    "vite-plugin-style-import": "^2.0.0",
    "vue-tsc": "^0.31.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marsgis/mars2d-vue-template.git"
  },
  "bugs": {
    "url": "https://github.com/marsgis/mars2d-vue-template/issues",
    "email": "wh@marsgis.cn"
  },
  "keywords": [
    "marsgis",
    "mars2d",
    "leaflet",
    "vue",
    "vue3",
    "gis",
    "webgis"
  ],
  "author": "火星科技",
  "license": "Apache-2.0",
  "homepage": "http://mars2d.cn"
}
